fun-def    ::= (def <id> (<id>*) <exp>)  ; A function definition
exp        ::= (<id> <exp>*)             ; A function call
             | <num>
             | <id>
num        ::= 0-9
alpha      ::= a-z | A-Z
id         ::= <id-char>+
id-char    ::= ? | ^ | * | + | = | <alpha>
             | < | > | - | _ | <num>
